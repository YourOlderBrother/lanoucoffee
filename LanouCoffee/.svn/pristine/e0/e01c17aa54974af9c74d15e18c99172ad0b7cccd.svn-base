package com.lanou.model;

public class Admin {
	private Integer admId;//管理员的id
	private Integer admAccount;//管理员的账户(手机号)
	private String admPassword;//管理员的密码
	private String admName;//管理员的姓名
	
	public Admin() {
		super();
	}

	public Admin(Integer admAccount, String admPassword, String admName) {
		super();
		this.admAccount = admAccount;
		this.admPassword = admPassword;
		this.admName = admName;
	}

	public Integer getAdmId() {
		return admId;
	}

	public void setAdmId(Integer admId) {
		this.admId = admId;
	}

	public Integer getAdmAccount() {
		return admAccount;
	}

	public void setAdmAccount(Integer admAccount) {
		this.admAccount = admAccount;
	}

	public String getAdmPassword() {
		return admPassword;
	}

	public void setAdmPassword(String admPassword) {
		this.admPassword = admPassword;
	}

	public String getAdmName() {
		return admName;
	}

	public void setAdmName(String admName) {
		this.admName = admName;
	}
	
}
