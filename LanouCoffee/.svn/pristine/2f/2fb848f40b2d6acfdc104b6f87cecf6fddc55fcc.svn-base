package com.lanou.model;

public class Coffee {
	private Integer cofId;//商品ID
	private String cofName;//商品名
	private String Kind;//商品类
	private String Introduce;//商品简介
	private Double Price;//商品价格
	private String Img;//商品图片
	
	public Coffee() {
		super();
	}

	public Coffee(String cofName, String kind, String introduce, Double price, String img) {
		super();
		this.cofName = cofName;
		Kind = kind;
		Introduce = introduce;
		Price = price;
		Img = img;
	}

	public Integer getCofId() {
		return cofId;
	}

	public void setCofId(Integer cofId) {
		this.cofId = cofId;
	}

	public String getCofName() {
		return cofName;
	}

	public void setCofName(String cofName) {
		this.cofName = cofName;
	}

	public String getKind() {
		return Kind;
	}

	public void setKind(String kind) {
		Kind = kind;
	}

	public String getIntroduce() {
		return Introduce;
	}

	public void setIntroduce(String introduce) {
		Introduce = introduce;
	}

	public Double getPrice() {
		return Price;
	}

	public void setPrice(Double price) {
		Price = price;
	}

	public String getImg() {
		return Img;
	}

	public void setImg(String img) {
		Img = img;
	}

	
	
}
