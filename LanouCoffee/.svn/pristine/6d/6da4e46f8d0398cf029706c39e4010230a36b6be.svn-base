*{
			margin: 0px;
			padding: 0px;
		}
		/*导航栏*/
		.head-border{
			border-radius: 5px;
			background-color: #F3F3F3;
			width: 1141px;
			height: 63px;
			margin: auto;
			margin-top: 36px;
		}
		.logo-border{
			border-top-left-radius: 5px;
			border-bottom-left-radius: 4px;
			/*background-color: red;*/
			width: 220px;
			float: left;
			display: inline-block;
		}
		.feature-link{}
		.logo-link,.feature-link,.menus-link,.message-link,.help-link{
			text-decoration: none;
		}
		.logo{
			color: #000000;
			margin-left: 5px;
			line-height: 63px;
		}
		.feature,.menus,.message,.help{
			line-height: 63px;
			/*color: white;*/
			margin-left: 60px;
		}
		.logo,.feature,.message,.help{
			color: white;
		}
		.feature-border,.message-border{
			float: left;
			/*background-color: #3D3D3D;*/
			width: 220px;
		}
		.menus-border,.help-border{
			float: left;
			/*background-color: #454545;*/
			width: 220px;
		}
		.help-border{
			border-top-right-radius: 5px;
			border-bottom-right-radius: 4px;
			width: 260px;
		}
		.logo-border,.help-border{
			background-color: #3D3D3D;
		}
		.message-border,.feature-border{
			background-color: #454545;
		}
		.menus-border{
			background-color: red;
		}
		/*中间内容*/
		.center-border{
			width: 1141px;
			height: 100%;
			margin: auto;
			margin-top: 20px;
		}
		.big{
				width: 200px;
				height: 300px;
				position: relative;
				/*margin: auto;*/
				
			}
			.left{
				width: 100px;
				height: 300px;
				background-color: #000000;
				text-align: center;
				display: inline-block;
			}
			.tm1,.tm2,.tm3,.tm4,.tm5,.tm6{
				width: 100px;
				height: 50px;
				line-height: 50px;
				color: white;
			}
			.one{
				width: 100px;
				height: 300px;
				background-color: red;
				text-align: center;
				position: absolute;
				top: 0px;
				left: 100px;
				visibility: hidden;
			}
			.tm11,.tm12,.tm13,.tm14{
				width: 100px;
				height: 50px;
				line-height: 50px;
				color: white;
			}
			.tm11 a,.tm12 a,.tm13 a,.tm14 a{
				color: white;
				/*text-underline: none;*/
			}
			.tm1:hover .one{
				visibility: visible;
			}
			.tm1:hover .one a:hover{
				font-size: 25px;
			}
			.tm2:hover .one{
				visibility: visible;
			}
			.tm2:hover .one a:hover{
				font-size: 25px;
			}
			.tm3:hover .one{
